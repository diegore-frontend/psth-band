.col1, .col2, .col3, .col4, .col5, .col6,
 .col7, .col8, .col9, .col10, .col11, .col12
	float left
	min-height 1px
	position relative
	margin-left 2%
	
	&:first-child
		margin-left 0px
		position relative

// #Grid
.col1 
	width 6.5%
.col2 
	width 15%
.col3 
	width 23.5%
.col4 
	width 32%
.col5 
	width 40.5%
.col6 
	width 49%
.col7 
	width 57.5%
.col8 
	width 66%
.col9 
	width 74.5%
.col10 
	width 83%
.col11 
	width 91.5%
.col12 
	width 100% 

// #Clear
.row:before,.row:after,.clearfix:before,.clearfix:after, cf:after
	content ''
	display table

.row:after,.clearfix:after, .cf:after
	clear both
	content ' '
	display block
	font-size 0
	line-height 0
	visibility hidden
	width 0
	height 0
	
